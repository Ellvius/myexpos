// For init process

PTBR = PAGE_TABLE_BASE + 20;
PTLR = 10;

// Library
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100";

// Heap
[PTBR+4] = -1;
[PTBR+5] = "0000";
[PTBR+6] = -1;
[PTBR+7] = "0000";

// Code
[PTBR+8] = 65;
[PTBR+9] = "0100";
[PTBR+10] = -1;
[PTBR+11] = "0000";
[PTBR+12] = -1;
[PTBR+13] = "0000";
[PTBR+14] = -1;
[PTBR+15] = "0000";

// Stack
[PTBR+16] = 66;
[PTBR+17] = "0110";
[PTBR+18] = -1;
[PTBR+19] = "0000";

alias PROCESS R1;
PROCESS = PROCESS_TABLE + INIT_PROCESS * 16;

// Process table for init process
[PROCESS+ 1] = 1;                           // PID
[PROCESS+ 3] = 1;                           // USERID
[PROCESS+ 4] = CREATED;                     // Status
[PROCESS+ 11] = 77;                         // User area page
[PROCESS+ 12] = 0;                          // KPTR
[PROCESS+ 13] = 8 * 512;                    // UPTR
[PROCESS+ 14] = PAGE_TABLE_BASE + 20;       // PTBR
[PROCESS+ 15] = 10;                         // PTLR

[66*512] = [65*512 + 1];

alias i R0;
// Initialize the per process table for init 
i = 0;
while(i < 16) do
    [[PROCESS+ 11] * 512 + RESOURCE_TABLE_OFFSET + i] = -1;
    i = i + 1;
endwhile;


// For shell process

PTBR = PAGE_TABLE_BASE + 40;
PTLR = 10;

// Library
[PTBR+0] = 63;
[PTBR+1] = "0100";
[PTBR+2] = 64;
[PTBR+3] = "0100";

// Heap
[PTBR+4] = -1;
[PTBR+5] = "0000";
[PTBR+6] = -1;
[PTBR+7] = "0000";

// Code
[PTBR+8] = 67;
[PTBR+9] = "0100";
[PTBR+10] = 68;
[PTBR+11] = "0100";
[PTBR+12] = -1;
[PTBR+13] = "0000";
[PTBR+14] = -1;
[PTBR+15] = "0000";

// Stack
[PTBR+16] = 78;
[PTBR+17] = "0110";
[PTBR+18] = 79;
[PTBR+19] = "0110";

PROCESS = PROCESS_TABLE + SHELL_PROCESS * 16;

// Process table for shell process
[PROCESS + 1] = SHELL_PROCESS;               // PID
[PROCESS + 2] = LOGIN_PROCESS;               // PPID
[PROCESS + 3] = 1;                           // USERID
[PROCESS + 4] = TERMINATED;                  // Status
[PROCESS + 11] = 80;                         // User area page
[PROCESS + 12] = 0;                          // KPTR
[PROCESS + 13] = 8 * 512;                    // UPTR
[PROCESS + 14] = PAGE_TABLE_BASE + 40;       // PTBR
[PROCESS + 15] = 10;                         // PTLR

[78*512] = [67*512 + 1];

// Set up the Disk Map table for shell
alias diskMapTable R1;
diskMapTable = DISK_MAP_TABLE + SHELL_PROCESS * 10;

i = 0;
while(i < 10) do
    [diskMapTable + i] = -1;
    i = i + 1;
endwhile;

[diskMapTable + 4] = 9;
[diskMapTable + 5] = 10;


// Terminal Status Table
[TERMINAL_STATUS_TABLE] = 0;

// Disk Status Table
[DISK_STATUS_TABLE] = 0;

[SYSTEM_STATUS_TABLE + 2] = 45;     // Mem free count
[SYSTEM_STATUS_TABLE + 3] = 0;      // Wait mem count
[SYSTEM_STATUS_TABLE + 4] = 0;      // Swapped count
[SYSTEM_STATUS_TABLE + 5] = 0;      // Paging Status


// Initialize memory free list
i = 0;

while(i < 128) do
    if (i < 81) then
        [MEMORY_FREE_LIST + i] = 1;
    else
        [MEMORY_FREE_LIST + i] = 0;
    endif;
    i = i + 1;
endwhile;


// Initialize Semaphore table
i = 0;

while(i < MAX_SEM_COUNT) do
    [SEMAPHORE_TABLE + i * 4] = -1;
    [SEMAPHORE_TABLE + i * 4 + 1] = 0;
    i = i + 1;
endwhile;


// Initialize File status table
i = 0;

while(i < MAX_FILE_NUM) do
    [FILE_STATUS_TABLE + i * 4] = -1;
    [FILE_STATUS_TABLE + i * 4 + 1] = -1;
    i = i + 1;
endwhile;


// Initialize Buffer table
i = 0;

while(i < MAX_BUFFER) do
    [BUFFER_TABLE + i * 4] = -1;
    [BUFFER_TABLE + i * 4 + 1] = 0;
    [BUFFER_TABLE + i * 4 + 2] = -1;
    i = i + 1;
endwhile;


// Initialize Open File table
i = 0;

while(i < MAX_OPENFILE_NUM) do
    [OPEN_FILE_TABLE + i * 4] = -1;
    [OPEN_FILE_TABLE + i * 4 + 1] = 0;
    i = i + 1;
endwhile;


// Set status of other process to TERMINATED
i = 0;

while(i < MAX_PROC_NUM - 1) do
    if(i > 1) then
        [PROCESS_TABLE +  i*16 + 4] = TERMINATED;
    endif;
    [PROCESS_TABLE + i * 16] = 0;
    i= i + 1;
endwhile;


return;